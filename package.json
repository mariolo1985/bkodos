{
    "name": "bkodos",
    "version": "0.0.1",
    "description": "beverlykodos",
    "main": "index.js",
    "scripts": {
      "production": "webpack -p",
      "start": "webpack-dev-server",
      "build:component": "babel ./src/components --out-dir ./build",
      "build:cssdev": "sass ./src/scss/master.scss ./src/css/master.min.css",
      "build:css": "sass ./src/scss/master.scss ./src/css/master.min.css -t compressed",
      "build:packdev": "webpack --progress",
      "build:pack": "webpack --progress -p",
      "build:dev": "yarn build:cssdev && yarn build:packdev",
      "build": "yarn build:css && yarn build:component && yarn build:pack"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/mariolo1985/bkodos.git"
    },
    "keywords": [
      "reactjs"
    ],
    "author": "M Lo",
    "license": "ISC",
    "bugs": {
      "url": "https://github.com/mariolo1985/bkodos/issues"
    },
    "homepage": "https://github.com/mariolo1985/bkodos#readme",
    "dependencies": {
      "react": "^15.0.2",
      "react-dom": "^15.0.2"
    },
    "devDependencies": {
      "babel-core": "^6.7.7",
      "babel-loader": "^6.2.4",
      "babel-preset-es2015": "^6.24.1",
      "babel-preset-react": "^6.5.0",
      "babel-preset-stage-1": "^6.24.1",
      "eslint": "^4.19.1",
      "html-webpack-plugin": "^2.16.0",
      "prop-types": "^15.6.1",
      "webpack": "^1.13.0",
      "webpack-dev-server": "^1.14.1"
    }
  }