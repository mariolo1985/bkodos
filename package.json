{
    "name": "bkodos",
    "version": "0.0.1",
    "description": "beverlykodos",
    "main": "index.js",
    "scripts": {
        "production": "webpack -p",
        "start": "webpack-dev-server",
        "build:component": "babel ./src/components --out-dir ./src/build",
        "build:cssdev": "yarn build:cssdev:master && yarn build:cssdev:index && yarn build:cssdev:about",
        "build:cssdev:master": "sass ./src/scss/master.scss ./src/css/master.min.css",
        "build:cssdev:index": "sass ./src/scss/index.scss ./src/css/index.min.css",
        "build:cssdev:about": "sass ./src/scss/about.scss ./src/css/about.min.css",
        "build:css": "yarn build:css:master && yarn build:css:index && yarn build:css:about",
        "build:css:master": "sass ./src/scss/master.scss ./src/css/master.min.css -t compressed",
        "build:css:index": "sass ./src/scss/index.scss ./src/css/index.min.css -t compressed",
        "build:css:about": "sass ./src/scss/about.scss ./src/css/about.min.css -t compressed",
        "build:packdev": "webpack --progress",
        "build:pack": "webpack --progress -p",
        "build:dev": "yarn build:cssdev && yarn build:component && yarn build:packdev",
        "build": "yarn build:css && yarn build:component && yarn build:pack"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mariolo1985/bkodos.git"
    },
    "keywords": [
        "reactjs"
    ],
    "author": "M Lo",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/mariolo1985/bkodos/issues"
    },
    "homepage": "https://github.com/mariolo1985/bkodos#readme",
    "dependencies": {
        "react": "^16.3.1",
        "react-dom": "^16.3.1"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.4",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-1": "^6.24.1",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^17.0.0",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.10.0",
        "prop-types": "^15.6.1",
        "webpack": "^3.8.1"
    }
}